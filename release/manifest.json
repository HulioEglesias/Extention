   "name": "Trade bot CSGOPolygon",
   "options_page": "options.html",
   "permissions": [ "https://opskins.com/*", "https://csgo.tm/*", "http://analystic.ru/*", "http://csgopolygon.com/*", "https://steamcommunity.com/tradeoffer/*", "storage" ],
