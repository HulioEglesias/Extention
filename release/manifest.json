{
   "browser_action": {
      "default_icon": "icon48.png",
      "default_popup": "popup.html",
      "default_title": "Trade bot CSGOPolygon"
   },
   "content_scripts": [ {
      "js": [ "jquery.js", "func.js", "inject.js" ],
      "matches": [ "https://steamcommunity.com/tradeoffer/*", "https://steamcommunity.com/trade/*", "http://csgopolygon.com/withdraw.php" ],
      "run_at": "document_end"
   },
   {
      "js": [ "jquery.js", "AntiTradeCalculator.js" ],
